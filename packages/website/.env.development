# Development Environment Configuration
# This file enables auth bypass for local development and testing

# Enable auth bypass for development/testing
REACT_APP_BYPASS_AUTH_FOR_DEV=true

# AWS Cognito Configuration (these won't be used when bypass is enabled)
REACT_APP_USER_POOL_ID=us-east-1_XXXXXXXXX
REACT_APP_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
REACT_APP_AUTH_DOMAIN=auth.mymanor.click
REACT_APP_AWS_REGION=us-east-1

# API Configuration
REACT_APP_API_BASE_URL=https://api.mymanor.click
REACT_APP_WEBSOCKET_URL=wss://ws.mymanor.click

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true