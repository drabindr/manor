# Test Environment Configuration
# This file is specifically for running automated tests

# Enable auth bypass for all tests
REACT_APP_BYPASS_AUTH_FOR_DEV=true

# Mock AWS Cognito Configuration (not used when bypass is enabled)
REACT_APP_USER_POOL_ID=us-east-1_TESTPOOL
REACT_APP_USER_POOL_CLIENT_ID=test-client-id-for-testing
REACT_APP_IDENTITY_POOL_ID=us-east-1:test-identity-pool-id
REACT_APP_AUTH_DOMAIN=test-auth.example.com
REACT_APP_AWS_REGION=us-east-1

# Mock API Configuration for testing
REACT_APP_API_BASE_URL=https://test-api.example.com
REACT_APP_WEBSOCKET_URL=wss://test-ws.example.com

# Test Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true
REACT_APP_TEST_MODE=true