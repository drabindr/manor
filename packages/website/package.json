  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "start": "vite --host",
    "build": "vite build",
    "build:production": "cp .env.production .env && npm run build",
    "test": "echo \"No test specified\" && exit 0",
    "predeploy": "npm run build:production",
    "deploy": "cd ../cdk && cdk deploy --require-approval never ManorWebsiteStack",
