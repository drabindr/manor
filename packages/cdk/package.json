{
  "name": "@manor/cdk",
  "version": "0.1.0",
  "bin": {
    "casa-guard-cdk": "dist/bin/casa-guard-cdk.js"
  },
  "scripts": {
    "prebuild": "if [ \"$SKIP_PREBUILD\" != \"true\" ]; then cd ../website && npm run build:production; fi",
    "build": "tsc",
    "build:ci": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && npx cdk deploy --require-approval never --progress events ManorWebsiteStack",
    "clean": "find . -type f -not -path \"./node_modules/*\" -not -path \"./dist/*\" \\( -name \"*.js\" -o -name \"*.d.ts\" \\) -delete"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.540.0",
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.19",
    "aws-cdk": "2.1017.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.758.0",
    "@aws-sdk/client-ssm": "^3.121.0",
    "@aws-sdk/lib-dynamodb": "^3.758.0",
    "@types/ws": "^8.5.12",
    "aws-cdk-lib": "^2.133.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.7.9",
    "constructs": "^10.3.0",
    "google-auth-library": "^9.14.1",
    "jsonwebtoken": "^9.0.2",
    "node-apn": "^3.0.0",
    "source-map-support": "^0.5.21",
    "uuid": "^11.0.5",
    "ws": "^8.18.2"
  }
}
